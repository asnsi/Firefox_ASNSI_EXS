{
  "manifest_version": 2,
  "name": "Protect+ (Internet Protection)",
  "version": "1.0",
  "description": "L'extension v√©rifie les sites Internet avant de les ouvrir pour vous.",
  "default_locale": "fr",
  "icons": {
    "48": "icons/unf-48.png",
    "96": "icons/unf-96.png"
  },
  "browser_action": {
    "default_icon": "icons/unf-32.png",
    "default_title": "Protect+",
    "default_popup": "popup/index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "contextMenus",
    "https://api.npoint.io/*"
  ]
}